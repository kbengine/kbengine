#include "sigar.h"

static sigar_version_t sigar_version = {
    "@@BUILD_DATE@@",
    "@@SCM_REVISION@@",
    "@@VERSION_STRING@@",
    "@@ARCHNAME@@",
    "@@ARCHLIB@@",
    "@@BINNAME@@",
    "SIGAR-@@VERSION_STRING@@, "
    "SCM revision @@SCM_REVISION@@, "
    "built @@BUILD_DATE@@ as @@ARCHLIB@@",
    @@VERSION_MAJOR@@,
    @@VERSION_MINOR@@,
    @@VERSION_MAINT@@,
    @@VERSION_BUILD@@
};

SIGAR_DECLARE(sigar_version_t *) sigar_version_get(void)
{
    return &sigar_version;
}
