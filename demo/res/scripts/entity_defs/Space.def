<root>
	<Volatile>
		<position/>
		<yaw/>
		<pitch> 20 </pitch>
	</Volatile>

	<Implements>
		<Interface>			GameObject		</Interface>
	</Implements>

	<Properties>
		<spaceUType>
			<Type>			UINT32				</Type>
			<Flags>			CELL_PRIVATE			</Flags>
			<Persistent>		false				</Persistent>
		</spaceUType>

		<spaceKey>
			<Type>			DBID				</Type>
			<Flags>			BASE				</Flags>
			<Persistent>		true				</Persistent>
		</spaceKey>

		<context>
			<Type>			PYTHON				</Type>
			<Flags>			BASE				</Flags>
			<Persistent>		true				</Persistent>
		</context>
	</Properties>

	<BaseMethods>
		<loginToSpace>
			<Arg>		MAILBOX		</Arg>
			<Arg>		PYTHON		</Arg>
		</loginToSpace>

		<logoutSpace>
			<Arg>		ENTITY_ID	</Arg>
		</logoutSpace>

		<teleportSpace>
			<Arg>		MAILBOX		</Arg>
			<Arg>		ENTITY_UTYPE	</Arg>
			<Arg>		POSITION3D	</Arg>
			<Arg>		DIRECTION3D	</Arg>
			<Arg>		PYTHON		</Arg>
		</teleportSpace>

		<onEnter>
			<Arg>		MAILBOX		</Arg>
		</onEnter>

		<onLeave>
			<Arg>		ENTITY_ID	</Arg>
		</onLeave>
	</BaseMethods>

	<CellMethods>
		<onEnter>
			<Arg>		MAILBOX		</Arg>
		</onEnter>

		<onLeave>
			<Arg>		ENTITY_ID	</Arg>
		</onLeave>
	</CellMethods>
</root>
